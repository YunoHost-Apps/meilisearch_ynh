{
  "name": "MeiliSearch",
  "id": "meilisearch",
  "packaging_format": 1,
  "description": {
    "en": "Lightning Fast, Ultra Relevant, and Typo-Tolerant Search Engine.",
    "fr": "Moteur de recherche rapide, ultra-performant et tolérant aux fautes de frappe."
  },
  "version": "0.10.1~ynh1",
  "url": "https://www.meilisearch.com/",
  "license": "free",
  "maintainer": {
    "name": "Julien Gomes Dias",
    "email": "abld@abld.info",
    "url": "https://julien.gomes-dias.dev"
  },
  "requirements": {
    "yunohost": ">= 3.5"
  },
  "multi_instance": true,
  "services": [
    "nginx"
  ],
  "arguments": {
    "install" : [
      {
        "name": "domain",
        "type": "domain",
        "ask": {
          "en": "Choose a domain name for Meilisearch",
          "fr": "Choisissez un nom de domaine pour Meilisearch"
        },
        "example": "example.com"
      },
      {
        "name": "path",
        "type": "path",
        "ask": {
          "en": "Choose a path for Meilisearch",
          "fr": "Choisissez un chemin pour Meilisearch"
        },
        "example": "/mailisearch",
        "default": "/mailisearch"
      },
      {
        "name": "is_public",
        "type": "boolean",
        "ask": {
          "en": "Is it a public application?",
          "fr": "Est-ce une application publique ?"
        },
        "default": true
      },
      {
        "name": "master_key",
        "ask": {
          "en": "Choose a master API key for Meilisearch",
          "fr": "Choisissez un nom de domaine pour Meilisearch"
        },
        "example": "API-key245678635248795"
      },
      {
        "name": "data_path",
        "type": "path",
        "ask": {
          "en": "Choose a path (existing or not) for Meilisearch data files (absolute path)",
          "fr": "Choisissez un dossier existant ou non pour les fichiers de données de Meilisearch (chemin absolu)"
        },
        "example": "/var/www/mailisearch",
        "default": "/var/www/mailisearch"
      }
    ]
  }
}
